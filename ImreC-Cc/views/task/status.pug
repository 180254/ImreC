extends ../base/layout.pug

block content
    h3 Resize commission status
    p
        ul
            li ID = #{id}
            li URL = #[a(href=url)=url]
            li Scale = #{scale}%
            li Worker = #{worker}
            li Status = #[b=status]

    a(href="#" class="reload") Refresh (#[b F5]).
    br
    a(href="/") Go back.