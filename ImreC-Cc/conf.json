{
  "SimpleDb": {
    "Domain": "imrec_sdb",
    "LogItemName": "__Log"
  },
  "Sqs": {
    "Url": "https://sqs.us-west-2.amazonaws.com/079592274660/Imrec-SQS"
  },
  "S3": {
    "Name": "imrec-bucket",
    "Url": "https://imrec-bucket.s3-us-west-2.amazonaws.com",
    "Policy": {
      "expiration": {"day": 1},
      "conditions": [
        ["starts-with", "$key", ""],
        {"bucket": "imrec-bucket"},
        {"acl": "private"},
        {"success_action_redirect": "/task/schedule"},
        ["content-length-range", 1, 5242880],
        ["eq", "$x-amz-meta-workstatus", "0"],
        ["eq", "$x-amz-meta-uploadedby", ""],
        ["starts-with", "$x-amz-meta-ofilename", ""],
        ["starts-with", "$x-amz-meta-newsize", ""]
      ]
    }
  }
}